{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "EvoAPI.Core.Services.AuditService": "Information",
      "EvoAPI.Api.Middleware.AuditMiddleware": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=tcp:evotest.database.windows.net,1433;Initial Catalog=EVO;User ID=EvoUser;Password={DB_PASSWORD};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  },
  "GoogleMaps": {
    "ApiKey": "${GOOGLE_MAPS_API_KEY}"
  },
  "Fleetmatics": {
    "BaseUrl": "${FLEETMATICS__BASEURL}",
    "Username": "${FLEETMATICS__USERNAME}",
    "Password": "${FLEETMATICS__PASSWORD}",
    "AtmosphereAppId": "${FLEETMATICS__ATMOSPHEREAPPID}",
    "SyncHour": 2
  }
}
