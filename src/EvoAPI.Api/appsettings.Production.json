{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "EvoAPI.Core.Services.AuditService": "Information",
      "EvoAPI.Api.Middleware.AuditMiddleware": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=tcp:evoprod.database.windows.net,1433;Initial Catalog=EVO;User ID=EvoUser;Password={DB_PASSWORD};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  },
  "GoogleMaps": {
    "ApiKey": "${GOOGLE_MAPS_API_KEY}"
  },
  "Fleetmatics": {
    "BaseUrl": "${FLEETMATICS_BASE_URL}",
    "Username": "${FLEETMATICS_USERNAME}",
    "Password": "${FLEETMATICS_PASSWORD}",
    "SyncHour": 2
  }
}
